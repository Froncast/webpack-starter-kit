    "webpack-starter-kit",
    "js",
    "javascript"
  ],
  "author": "Oleg Tretyakov <ichimaru859@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Froncast/webpack-starter-kit/issues"
  },
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
